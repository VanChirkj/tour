     <bool name="tr:Interact with tour info events" id="listen_info_event" uiPreview="false" customize="false" default="true"/>
    </param>
    <param id="" uiPreview="false" customize="false">
     <group name="tr:User interface" id="advanced_ui" uiPreview="false" customize="false">
      <bool name="tr:Display a close button" id="display_close" uiPreview="false" customize="false" default="false"/>
      <string needTranslation="true" name="tr:Close tooltip" id="display_close_text" conditionalParam="display_close" conditionalValues="true" uiPreview="false" customize="false" default="Close"/>
